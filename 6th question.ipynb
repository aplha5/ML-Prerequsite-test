   "source": [
    "import sympy as sp\n",
    "x,y=sp.symbols('x,y')\n",
    "func=x**2*y+y**3*(sp.sin(x))\n",
    "gradient=sp.Matrix([func.diff(x),func.diff(y)])\n",
    "print (\"Gradient :\",gradient)"
